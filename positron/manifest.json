{
  "manifest_version": 2,

  "name": "Positron",
  "description": "",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Start Positron"
  },

  "content scripts": [{
    "js": ["js/lib", "js/app"],
    "matches": ["*://*/*"],
    "run at": "document_start"
    }],
  "minimum_chrome_verstion": "18",
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
